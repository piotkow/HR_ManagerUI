{
  "name": "app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:app": "node dist/app/server/server.mjs",
    "swagger:download": "node libs/download-spec.js",
    "swagger:generate": "openapi-generator-cli generate -g typescript-angular --type-mappings=set=Array --additional-properties=serviceSuffix=Api,serviceFileSuffix=.api,fileNaming=kebab-case,useSingleRequestParameter=true,useTags=true -i ./libs/openapi.json -o ./libs/api-client/",
    "swagger": "npm run swagger:download && npm run swagger:generate"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.0",
    "@angular/common": "^17.1.0",
    "@angular/compiler": "^17.1.0",
    "@angular/core": "^17.1.0",
    "@angular/forms": "^17.1.0",
    "@angular/platform-browser": "^17.1.0",
    "@angular/platform-browser-dynamic": "^17.1.0",
    "@angular/platform-server": "^17.1.0",
    "@angular/router": "^17.1.0",
    "@angular/ssr": "^17.1.2",
    "@auth0/angular-jwt": "^5.2.0",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@openapitools/openapi-generator-cli": "^2.11.0",
    "@popperjs/core": "^2.11.8",
    "angular-calendar": "^0.31.0",
    "bootstrap": "^5.3.2",
    "express": "^4.18.2",
    "font-awesome": "^4.7.0",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primeng": "^17.8.0",
    "rxjs": "~7.8.0",
    "tooltip": "^1.6.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.2",
    "@angular/cli": "^17.1.2",
    "@angular/compiler-cli": "^17.1.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.3.2"
  }
